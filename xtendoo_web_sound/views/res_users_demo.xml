<odoo>
    <record id="view_users_form_simple_modif_inherit" model="ir.ui.view">
        <field name="name">res.users.form.simple.view.modif.inherit</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[1]" position="after">
                <page string="Test web sound" name="test_web_sound">
                    <button name="button" class="button_widget" string="ok"/>

                                      
                </page>
            </xpath>
        </field>
    </record>

    <record id="quiz_action" model="ir.actions.client">
        <field name="name">Quiz Action</field>
        <field name="tag">quiz_action</field>
    </record> 

</odoo>
